{
    "name": "@christianmaf80/agentic-workflow",
    "version": "1.18.4-beta.3",
    "description": "Portable agentic workflow orchestration system with strict identity and gate discipline",
    "type": "module",
    "displayName": "Agentic Workflow VSCode",
    "main": "./dist/extension.js",
    "engines": {
        "vscode": "^1.108.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:agentic-workflow.helloWorld"
    ],
    "contributes": {
        "commands": [
            {
                "command": "agentic-workflow.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "bin": {
        "agentic-workflow": "bin/cli.js"
    },
    "files": [
        "dist/cli",
        "dist/core",
        "dist/rules",
        "dist/workflows",
        "dist/templates",
        "dist/artifacts",
        "dist/index.md",
        "bin",
        "README.md",
        "LICENSE",
        "RELEASE_PROCESS.md"
    ],
    "scripts": {
        "build": "node scripts/clean-dist.mjs && tsc && npm run copy-assets",
        "copy-assets": "cp -r src/agentic-system-structure/rules src/agentic-system-structure/workflows src/agentic-system-structure/templates src/agentic-system-structure/artifacts dist/ && cp src/agentic-system-structure/index.md dist/index.md",
        "start": "node dist/index.js",
        "publish:npm": "npm publish --registry https://registry.npmjs.org --access public",
        "publish:github": "npm publish --registry https://npm.pkg.github.com",
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test",
        "package": "npm pack && mv *.tgz .backups/npm-packages/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/christian-marino-alvarez/agentic-workflow.git"
    },
    "bugs": {
        "url": "https://github.com/christian-marino-alvarez/agentic-workflow/issues"
    },
    "homepage": "https://github.com/christian-marino-alvarez/agentic-workflow#readme",
    "keywords": [
        "agent",
        "workflow",
        "orchestration",
        "ai",
        "llm",
        "agentic",
        "governance"
    ],
    "author": "Christian Marino Alvarez",
    "license": "ISC",
    "dependencies": {
        "@clack/prompts": "^0.7.0",
        "commander": "^11.1.0",
        "gray-matter": "^4.0.3"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "^20.10.6",
        "@types/vscode": "^1.108.1",
        "@vscode/test-cli": "^0.0.12",
        "@vscode/test-electron": "^2.5.2",
        "eslint": "^9.39.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.52.0"
    },
    "overrides": {
        "diff": "8.0.3"
    }
}
