{
  "name": "@christianmaf80/agentic-workflow",
  "version": "1.19.0-beta.13",
  "description": "Portable agentic workflow orchestration system with strict identity and gate discipline",
  "type": "module",
  "bin": {
    "agentic-workflow": "bin/cli.js"
  },
  "files": [
    "dist/cli",
    "dist/core",
    "dist/rules",
    "dist/workflows",
    "dist/templates",
    "dist/artifacts",
    "dist/index.md",
    "dist/extension",
    "bin",
    "README.md",
    "LICENSE",
    "RELEASE_PROCESS.md",
    "media"
  ],
  "scripts": {
    "build": "node scripts/clean-dist.mjs && tsc && npm run copy-assets",
    "copy-assets": "cp -r src/agentic-system-structure/rules src/agentic-system-structure/workflows src/agentic-system-structure/templates src/agentic-system-structure/artifacts dist/ && cp src/agentic-system-structure/index.md dist/index.md",
    "start": "node dist/index.js",
    "publish:npm": "npm publish --registry https://registry.npmjs.org --access public",
    "publish:github": "npm publish --registry https://npm.pkg.github.com",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "package": "npm pack && mv *.tgz .backups/npm-packages/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christian-marino-alvarez/agentic-workflow.git"
  },
  "bugs": {
    "url": "https://github.com/christian-marino-alvarez/agentic-workflow/issues"
  },
  "homepage": "https://github.com/christian-marino-alvarez/agentic-workflow#readme",
  "keywords": [
    "agent",
    "workflow",
    "orchestration",
    "ai",
    "llm",
    "agentic",
    "governance"
  ],
  "author": "Christian Marino Alvarez",
  "license": "ISC",
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "commander": "^11.1.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.10.6",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "@types/vscode": "^1.108.1"
  },
  "overrides": {
    "diff": "8.0.3"
  },
  "displayName": "Agentic Workflow",
  "main": "./dist/extension/extension.js",
  "engines": {
    "vscode": "^1.108.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:mainView"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "main",
          "title": "Agent Chat",
          "icon": "media/agent-chat.svg"
        }
      ]
    },
    "views": {
      "main": [
        {
          "id": "mainView",
          "name": "Agent Chat",
          "icon": "media/agent-chat.svg"
        }
      ]
    }
  }
}